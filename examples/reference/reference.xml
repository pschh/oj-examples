<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<oddjob id="this">
    <job>
        <sequential name="Reference Examples">
            <jobs>
                <mkdir dir="${this.dir}/work" name="Make Work Dir"/>
                <oddjob file="${this.dir}/job.ant.xml" name="ant"/>
                <oddjob file="${this.dir}/job.copy.xml" name="copy"/>
                <oddjob file="${this.dir}/job.delete.xml" name="delete"/>
                <oddjob file="${this.dir}/job.exec.xml" name="exec"/>
                <oddjob file="${this.dir}/job.exists.xml" name="exists"/>
                <oddjob file="${this.dir}/job.equals.xml" name="equals"/>
                <oddjob file="${this.dir}/job.foreach.xml" name="foreach"/>
                <oddjob file="${this.dir}/job.if.xml" name="if"/>
                <oddjob file="${this.dir}/job.launch.xml" name="launch"/>
                <oddjob file="${this.dir}/job.mkdir.xml" name="mkdir"/>
                <oddjob file="${this.dir}/job.oddjob.xml" name="oddjob"/>
                <oddjob file="${this.dir}/job.properties.xml" name="properties"/>
                <oddjob file="${this.dir}/job.rename.xml" name="rename"/>
                <oddjob file="${this.dir}/job.repeat.xml" name="repeat">
                    <classLoader>
                        <url-class-loader>
                            <files>
                                <file file="${this.dir}/../classes"/>
                            </files>
                        </url-class-loader>
                    </classLoader>
                </oddjob>
                <oddjob file="${this.dir}/job.schedule.xml" name="schedule"/>
                <oddjob file="${this.dir}/job.scheduler.xml" name="scheduler"/>
                <oddjob file="${this.dir}/job.sequence.xml" name="sequence"/>
                <oddjob file="${this.dir}/job.sequential.xml" name="sequential"/>
                <oddjob file="${this.dir}/job.sql.xml" name="sql"/>
                <oddjob file="${this.dir}/job.trigger.xml" name="trigger"/>
                <oddjob file="${this.dir}/job.unschedule.xml" name="unschedule"/>
                <oddjob file="${this.dir}/job.variables.xml" name="variables"/>
                <oddjob file="${this.dir}/job.wait.xml" name="wait"/>
            </jobs>
        </sequential>
    </job>
</oddjob>
